<div class="container">
  <%= link_to new_msl_gem_path do %>
    <button class="button">Add Gem</button>
  <% end %>

  <%= link_to dashboard_path do %>
    <button class="button">Back</button>
  <% end %>
</div>

<div class="container">
  <div class="gem-cards">
    <% @msl_gems.each do |msl_gem| %>
      <%= render "shared/astromon-gems", msl_gem: msl_gem %>
    <% end %>
  </div>
</div>
