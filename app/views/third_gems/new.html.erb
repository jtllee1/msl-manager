<%= simple_form_for([@astromon, @third_gem]) do |f| %>
  <div class="gem-cards">
  <% @third_gems.each do |msl_gem| %>
    <%= render "shared/eqastromon-gems", msl_gem: msl_gem %>
    <% if msl_gem.available %>
      <%= check_box_tag "msl_gem", msl_gem.id %>
    <% end %>
  <% end %>
  </div>
  <%= f.submit "Add Third Gem" %>
<% end %>
